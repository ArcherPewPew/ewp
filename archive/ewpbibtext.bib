@article{
 title = {The Economic Impacts of Large Wildfires},
 type = {article},
 year = {2013},
 pages = {23-27},
 websites = {https://icma.org/sites/default/files/305166_The Economic Impacts of Large Wildfires.pdf},
 id = {86e1f0a3-ec88-3ced-9090-0973769962d4},
 created = {2020-12-22T08:30:07.574Z},
 file_attached = {true},
 profile_id = {82b267fa-45c5-37e1-a7e3-d97f34cf2bbb},
