	pages = {109--123},
	file = {Snapshot:C\:\\Users\\adeak\\Zotero\\storage\\T2YETQTL\\19390459.2015.html:text/html},
}

@article{abrams_community_2016,
	title = {Community wildfire protection planning in the {American} {West}: homogeneity within diversity?},
