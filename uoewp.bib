	file = {Full Text PDF:C\:\\Users\\adeak\\Zotero\\storage\\E3BDXZZT\\Schultz et al. - 2018 - Key Findings and Recommendations Based on the USDA.pdf:application/pdf;Snapshot:C\:\\Users\\adeak\\Zotero\\storage\\836Q7V7J\\4747154.html:text/html},
}

@article{abrams_building_2017,
	title = {Building {Practical} {Authority} for {Community} {Forestry} in and through {Networks}: {The} role of community-based organisations in the {U}.{S}. {West}},
