@techreport{
 title = {Strategies for Supporting Frontline Collaboration: Lessons from Stewardship Contracting},
 type = {techreport},
 year = {2010},
 pages = {47},
 websites = {https://scholarsbank.uoregon.edu/xmlui/handle/1794/10994},
 city = {Washington, D.C.},
